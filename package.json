{
  "scripts": {
    "snowpack": "snowpack --dest src/web_modules",
    "postinstall": "npm run snowpack",
    "prebuild": "npm run snowpack",
    "build": "parcel build src/index.html",
    "format": "prettier --print-width 120 --write '{src,test}/**/*.js'",
    "start": "servor src index.html 8080 --reload --browse",
    "test": "mocha --require esm test/*.spec.js --timeout 5000",
    "test:coverage": "nyc npm run test && nyc report --reporter=html",
    "test:performance": "bundlesize"
  },
  "dependencies": {
    "@hyperapp/events": "0.0.3",
    "@kwasniew/hyperapp-fx": "2.0.0-beta.2",
    "classcat": "4.0.2",
    "htm": "2.1.1",
    "hyperapp": "2.0.4",
    "hyperapp-v2-basiclogger": "0.3.0",
    "page": "1.11.5",
    "snarkdown": "1.2.2",
    "snowpack": "1.6.0"
  },
  "devDependencies": {
    "@pollyjs/adapter-fetch": "2.6.3",
    "@pollyjs/adapter-node-http": "2.6.3",
    "@pollyjs/core": "2.6.3",
    "@pollyjs/persister-fs": "2.6.3",
    "@testing-library/dom": "6.4.1",
    "@testing-library/user-event": "7.0.1",
    "bundlesize": "0.18.0",
    "esm": "3.2.25",
    "isomorphic-unfetch": "3.0.0",
    "jsdom": "15.1.1",
    "jsdom-global": "3.0.2",
    "mocha": "6.1.4",
    "node-fetch": "2.6.0",
    "node-gzip": "1.1.2",
    "npm-run-all": "4.1.5",
    "nyc": "15.0.0 ",
    "parcel": "2.0.0-alpha.3.2",
    "prettier": "2.0.1",
    "servor": "^4.0.2",
    "undom": "0.4.0",
    "whatwg-fetch": "3.0.0"
  },
  "bundlesize": [
    {
      "path": "./dist/*.js",
      "maxSize": "20 kB"
    }
  ]
}
