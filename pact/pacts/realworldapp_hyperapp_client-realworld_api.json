{
  "consumer": {
    "name": "RealWorldApp Hyperapp Client"
  },
  "provider": {
    "name": "RealWorld API"
  },
  "interactions": [
    {
      "description": "request for tags",
      "providerState": "list of tags",
      "request": {
        "method": "GET",
        "path": "/api/tags"
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json"
        },
        "body": {
          "tags": [
            "tag"
          ]
        },
        "matchingRules": {
          "$.body.tags": {
            "min": 1
          },
          "$.body.tags[*].*": {
            "match": "type"
          }
        }
      }
    },
    {
      "description": "request for tags",
      "providerState": "failing tags",
      "request": {
        "method": "GET",
        "path": "/api/tags"
      },
      "response": {
        "status": 422,
        "headers": {
          "Content-Type": "application/json"
        },
        "body": {
          "errors": [
            "unexpected error"
          ]
        },
        "matchingRules": {
          "$.body.errors": {
            "min": 1
          },
          "$.body.errors[*].*": {
            "match": "type"
          }
        }
      }
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "2.0.0"
    }
  }
}