{
  "consumer": {
    "name": "RealWorldApp Hyperapp Client"
  },
  "provider": {
    "name": "RealWorld API"
  },
  "interactions": [
    {
      "description": "request for tags",
      "providerState": "list of tags",
      "request": {
        "method": "GET",
        "path": "/api/tags"
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json"
        },
        "body": {
          "tags": [
            "tag1",
            "tag2"
          ]
        }
      }
    },
    {
      "description": "request for tags",
      "providerState": "failing tags",
      "request": {
        "method": "GET",
        "path": "/api/tags"
      },
      "response": {
        "status": 422,
        "headers": {
          "Content-Type": "application/json"
        },
        "body": {
          "errors": {
            "body": [
              "unexpected error"
            ]
          }
        }
      }
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "2.0.0"
    }
  }
}